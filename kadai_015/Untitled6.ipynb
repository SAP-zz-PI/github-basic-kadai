        "class Human:\n",
        "    def __init__(self,name,age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    # メソッドを定義する\n",
        "    def printinfo(self):\n",
        "        print(\"名前は、\" + self.name + \"年齢は\" + str(self.age) + \"です。\")\n",
        "\n",
        "# インスタンス化する\n",
        "sasaki = Human(\"佐々木\",30)\n",
        "\n",
        "sasaki.printinfo()\n"
      ]
